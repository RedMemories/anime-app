{
  "expo": {
    "name": "AnimStream",
    "slug": "animstream",
    "version": "1.2.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/LogoAnimStream.png",
      "resizeMode": "contain",
      "backgroundColor": "#111111"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/LogoAnimStream.png",
        "backgroundColor": "#111111"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.animeapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-video"
    ]
  }
}
